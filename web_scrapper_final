    "# Try to find the first span\n",
    "price_span = new_soup.find(\"span\", attrs={\"class\": 'a-price a-text-price a-size-medium'})\n",
    "\n",
    "# Check if the first span is found\n",
    "if price_span:\n",
    "    # Find the second span inside the first one\n",
    "    price = price_span.find(\"span\", attrs={\"class\": \"a-offscreen\"})\n",
    "    if price:\n",
    "        print(price.text)\n",
    "    else:\n",
    "        print(\"Price span with class 'a-offscreen' not found.\")\n",
